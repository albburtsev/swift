<h1>Simple tile map</h1>

<div class="swiftmap" id="swiftmap"></div>

<script>
swift.Map('swiftmap');
</script>

<script type="text/plain">
// or 
new swift.Map('swiftmap');
</script>

<script type="text/plain">
// or
swift.Map( document.getElementById('swiftmap') );
</script>

<script type="text/plain">
// or
swift.Map( jQuery('#swiftmap').get(0) );
</script>